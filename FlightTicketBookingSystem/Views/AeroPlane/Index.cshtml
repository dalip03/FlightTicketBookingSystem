@model IEnumerable<FlightTicketBookingSystem.Models.AeroPlaneInfo>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayoutPage.cshtml";
}



<!-- main content start-->
<div id="page-wrapper">
    <div class="main-page">
        @*<h2 class="title1">Blank Page</h2>*@
        <div class="blank-page widget-shadow scroll" id="style-2 div1">

            <h2>Index</h2>

            <p>
                @Html.ActionLink("Create New", "Create")
            </p>
            <table class="table">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.APlaneName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.SeatingCapacity)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Price)
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.APlaneName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SeatingCapacity)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Price)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.Planeid }) |
                            @Html.ActionLink("Details", "Details", new { id = item.Planeid }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.Planeid })
                        </td>
                    </tr>
                }

            </table>
        </div>
    </div>
</div>